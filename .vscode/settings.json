{
  "i18n-ally.localesPaths": "scripts/locales",
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.sortKeys": true,
  "github.copilot.enable": {
    "typescript": false
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "pnpm-lock.yaml": true,
    "**/dist": true
  },
  "i18n-ally.keystyle": "flat",
  "typescript.tsdk": "node_modules/typescript/lib",
  "prettier.semi": false,
  "typescript.format.semicolons": "remove",
  "javascript.format.semicolons": "remove",
  "json.schemas": [
    {
      "fileMatch": [
        ".eemojirc.json",
        "package.json",
        ".eemojirc",
        ".eemojirc.yaml",
        ".eemojirc.yml",
        ".config/eemojirc",
        ".config/eemojirc.json",
        ".config/eemojirc.yaml",
        ".config/eemojirc.yml"
      ],
      "url": "https://rettend.github.io/eemoji/eemoji-config-schema.json"
    }
  ]
}